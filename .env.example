# 环境变量配置模板
# 复制此文件为 .env.local 并根据需要修改配置

# 基础应用信息
VITE_APP_TITLE=管理系统
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=基于Vue3和Ant Design Vue的管理系统
VITE_APP_ENV=development

# API配置
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# 应用配置
VITE_APP_DEBUG=true
VITE_APP_MOCK=true

# 服务配置
VITE_PORT=5174
VITE_HOST=localhost

# 日志配置
VITE_LOG_LEVEL=debug

# 功能开关
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_MOCK=true
VITE_ENABLE_VCONSOLE=true

# CDN配置（可选）
VITE_CDN_URL=

# 监控配置（可选）
VITE_SENTRY_DSN=
VITE_ANALYTICS_ID=
