# 测试环境配置
NODE_ENV=production

# API配置
VITE_API_BASE_URL=https://staging-api.yourdomain.com/api
VITE_API_TIMEOUT=20000

# 应用配置
VITE_APP_ENV=staging
VITE_APP_DEBUG=true
VITE_APP_MOCK=false

# 日志配置
VITE_LOG_LEVEL=warn

# 功能开关
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_MOCK=false
VITE_ENABLE_VCONSOLE=true

# CDN配置
VITE_CDN_URL=https://staging-cdn.yourdomain.com
